<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jsw">
  <select id="dual" resultType="Integer">
    select 1 from dual
  </select>
<!-- private int iNum;
	private String str;
   -->
  <select id="voselect" resultType="jsw.JswVO">
  	select 999 iNum, 'dbfield1' str from dual
  </select>
  
  <select id="listselect" resultType="jsw.JswVO">
  	select 999 iNum, 'dbfield1' str from dual union select  888 iNum, 'dbfield2' str from dual 
  </select>
  
  <select id="selectwhere1" resultType="String">
  select name from customer where id= #{name }
  </select>
  
  <select id="selectwhere2" resultType="customer.CustomerVO">
  		select * from customer where id= #{id} and name = #{name}
  </select>
  
  <select id="selectwhere3" resultType="customer.CustomerVO">
  		select * from customer where id>2
  </select>
  
  <select id="mapselect" resultType="String">
  	select CHARACTER_NAME from CHARACTERS
  	where CHARACTER_ID = #{id}
  	and CHARACTER_NAME = #{name}
  </select>
</mapper>